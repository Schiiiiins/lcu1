head	1.2;
access
	archiv
	jw
	wulff;
symbols;
locks; strict;
comment	@# @;


1.2
date	2005.06.12.17.09.36;	author jw;	state Exp;
branches;
next	1.1;

1.1
date	2005.04.22.10.58.08;	author jw;	state Exp;
branches;
next	;


desc
@initial
@


1.2
log
@rename a012 a1
@
text
@#!/bin/sh

########################################################################
#
#	a1.sh
#	This demo starts iCserver, 3 iCboxes and the control program
#	'a1' which is generated by iCmake a1.ic
#
#	All digital inputs X0.0 to X0.7 and analog inputs IB1 and IW2
#	act directly on the corresponding outputs.
#
#	IX0 is preset to 0x33, IB1 to 50 and IW2 to -10000.
#	The ranges of W2 have been changed from the default 0 to 100.
#
#	Stop the demo with ctrl-C
#
########################################################################

iCserver 'iCbox X0=0x33 B1=50 W2=-10000,-32568,32567' a1
@


1.1
log
@initial revision
@
text
@d5 1
a5 1
#	a012.sh
d7 1
a7 1
#	'a012' which is generated by iCmake a012.ic
d19 1
a19 1
iCserver 'iCbox X0=0x33 B1=50 W2=-10000,-32568,32567' a012
@
