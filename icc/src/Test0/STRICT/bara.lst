	m3_2    R ---#

	c0      : ---&  m3_1    S
	m8      ~ ---&
	m2        ---&

	c0      : ---&  m3_2    R
	m8        ---&
	m4      ~ ---&

045	m4 = SR(~m8 & m3, m8 & ~m5, c0);

	m4_1    S ---#  m4
	m4_2    R ---#

	c0      : ---&  m4_1    S
	m8      ~ ---&
	m3        ---&

	c0      : ---&  m4_2    R
	m8        ---&
	m5      ~ ---&

046	m5 = SR(~m8 & m4, m8 & ~m6, c0);

	m5_1    S ---#  m5
	m5_2    R ---#

	c0      : ---&  m5_1    S
	m8      ~ ---&
	m4        ---&

	c0      : ---&  m5_2    R
	m8        ---&
	m6      ~ ---&

047	m6 = SR(~m8 & m5, m8 & ~m7, c0);

	m6_1    S ---#  m6
	m6_2    R ---#

	c0      : ---&  m6_1    S
	m8      ~ ---&
	m5        ---&

	c0      : ---&  m6_2    R
	m8        ---&
	m7      ~ ---&

048	m7 = SR(~m8 & m6, m8      , c0);

