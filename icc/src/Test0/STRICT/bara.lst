	m7_2    R ---#

	c0      : ---&  m7_1    S
	m8      ~ ---&
	m6        ---&

	c0      : ---|  m7_2    R
	m8        ---|

049	m8 = SR(~m8 & m7, m8 & ~m0, c0);

	m8_1    S ---#  m8
	m8_2    R ---#

	c0      : ---&  m8_1    S
	m8      ~ ---&
	m7        ---&

	c0      : ---&  m8_2    R
	m8        ---&
	m0      ~ ---&

