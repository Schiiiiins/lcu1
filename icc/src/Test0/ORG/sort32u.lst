	                                            )==belt@mask8        // 2
	                                            ;                    // (5)

110	    gate8 = segment(car8, car9,    8, 12,  7, clk);

	belt@shift9_1 H ---*  belt@shift9   A
	belt@shift9_2 r ---*

	belt@clk      : ---+  belt@shift9_1 H
	belt@shift9_3 A ---+                        belt@shift9_3        // 1
