	                                            ;                    // (6)


	iConst        A ---+  belt@mask7    A       0x41<<7;             // (10)


	belt@shift7   A ---+  belt@car7     A       (belt@shift7         // 1
	                                            >>12)&1;             // (9)


	belt@gate6_1  S ---#  belt@gate6
	belt@gate6_2  R ---#

	belt@clk      : ---&  belt@gate6_1  S
	belt@gate6_3    ---&
	belt@gate6_4  ~ ---&

	belt@clk      : ---&  belt@gate6_2  R
	belt@gate6_3  ~ ---&
