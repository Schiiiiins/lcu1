011	use strict;
012
013	/********************************************************************
014	 *
015	 *	Feeder segment
016	 *
017	 *******************************************************************/
018
019	imm bit feeder(			/* feeds code into feeder segment */
020		imm bit transfer,	/* photo cell to transfer code */
021	 assign	imm int carryOut,	/* shift bit (as int) for the following segment */
022		imm int code,		/* destination code - 0 .. 9 */
023		imm int length,		/* sets the length of the segment */
024		imm int width,		/* width of lock frame 6 + 6 for 0x7f */
025		imm clock c,		/* stepping clock for the belt */
026	    )
027	{
028	    extern imm bit reset;	/* general re-initialisation */
029	    imm bit pip   = RISE(transfer & ~this & ~reset, c);

	feeder@pip_1    E ---/  feeder@pip

	feeder@c        : ---&  feeder@pip_1 E
	feeder@transfer   ---&
	feeder@         ~ ---&
	IX0.7           ~ ---&

030	    imm int shift = SHR((shift << 1) + (pip * (0x41 + (code << 1))), c, reset);

	feeder@shift_1 H ---*  feeder@shift   A
	feeder@shift_2 r ---*

	feeder@c       : ---+  feeder@shift_1 H
	feeder@shift_3 A ---+                         feeder@shift_3       // 1
	feeder@shift   A ---+                         ?feeder@shift        // 2
	feeder@shift_4 A ---+                         :feeder@shift_4      // 3
	                                              ;                    // (1)

	iClock         : ---|  feeder@shift_2 r
	IX0.7            ---|

	IX0.7            ---|  feeder@shift_3 A

	feeder@shift   A ---+  feeder@shift_4 A       (feeder@shift        // 1
	feeder@shift_5 A ---+                         <<1)+(feeder@shift_5 // 2
	feeder@code    A ---+                         *(0x41+(feeder@code  // 3
	                                              <<1)));              // (2)

	feeder@pip       ---|  feeder@shift_5 A

031	    imm int mask  = 0x41 << width;

	feeder@width A ---+  feeder@mask A       0x41<<feeder@width // 1
	                                         ;                  // (3)

032	    carryOut = (shift >> length) & 1;

	feeder@shift  A ---+  feeder@carryOut A       (feeder@shift   // 1
	feeder@length A ---+                          >>feeder@length // 2
	                                              )&1;            // (4)

033	    this = SRX(pip,		/* unlock after width steps */
034		       (shift & mask) == mask | reset, c);

	feeder@_1    S ---#  feeder@
	feeder@_2    R ---#

	feeder@c     : ---&  feeder@_1 S
	feeder@pip     ---&
	feeder@_3    ~ ---&

	feeder@c     : ---&  feeder@_2 R
	feeder@pip   ~ ---&
	feeder@_3      ---&

	feeder@_4      ---|  feeder@_3
	IX0.7          ---|

	feeder@shift A ---+  feeder@_4         (feeder@shift  // 1
	feeder@mask  A ---+                    &feeder@mask   // 2
	                                       )==feeder@mask // 2
	                                       ;              // (5)

035	}
036
037	/********************************************************************
038	 *
039	 *	Segment
040	 *
041	 *	Each segment controls one gate and may be up to 32 steps long
042	 *
043	 *******************************************************************/
044
045	imm bit segment(		/* returns gate control output */
046		imm int carryIn,	/* shift bit (as int) from the previous segment */
047	 assign	imm int carryOut,	/* shift bit (as int) for the following segment */
048		imm int code,		/* code identifying this segment */
049		imm int length,		/* segment length */
050		imm int width,		/* width of the gate */
051		imm clock c,		/* stepping clock for the belt */
052	    )
053	{
054	    extern imm bit reset;	/* general re-initialisation */
055	    imm int shift = SHR((shift << 1) + carryIn, c, reset);

	segment@shift_1 H ---*  segment@shift   A
	segment@shift_2 r ---*

	segment@c       : ---+  segment@shift_1 H
	segment@shift_3 A ---+                          segment@shift_3      // 1
	segment@shift   A ---+                          ?segment@shift       // 2
	segment@shift_4 A ---+                          :segment@shift_4     // 3
	                                                ;                    // (1)

	iClock          : ---|  segment@shift_2 r
	IX0.7             ---|

	IX0.7             ---|  segment@shift_3 A

	segment@shift   A ---+  segment@shift_4 A       (segment@shift       // 1
	segment@carryIn A ---+                          <<1)+segment@carryIn // 2
	                                                ;                    // (6)

056	    imm int mask  = 0x41 << width;

	segment@width A ---+  segment@mask A       0x41<<segment@width // 1
	                                           ;                   // (3)

057	    carryOut = (shift >> length) & 1;

	segment@shift  A ---+  segment@carryOut A       (segment@shift   // 1
	segment@length A ---+                           >>segment@length // 2
	                                                )&1;             // (4)

058	    this = SRX((shift & 0x7f) == 0x41 + (code << 1),
059		       (shift & mask) == mask | reset, c);

	segment@_1    S ---#  segment@
	segment@_2    R ---#

	segment@c     : ---&  segment@_1 S
	segment@_3      ---&
	segment@_4    ~ ---&

	segment@c     : ---&  segment@_2 R
	segment@_3    ~ ---&
	segment@_4      ---&

	segment@shift A ---+  segment@_3         (segment@shift             // 1
	segment@code  A ---+                     &0x7f)==0x41+(segment@code // 2
	                                         <<1);                      // (7)

	segment@_5      ---|  segment@_4
	IX0.7           ---|

	segment@shift A ---+  segment@_5         (segment@shift             // 1
	segment@mask  A ---+                     &segment@mask              // 2
	                                         )==segment@mask            // 2
	                                         ;                          // (5)

060	}
