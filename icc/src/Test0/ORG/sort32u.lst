062	/********************************************************************
063	 *
064	 *	Belt
065	 *
066	 *	Each belt has 10 gates
067	 *
068	 *******************************************************************/
069
070	imm int belt(
071	    	assign	imm bit lock,	/* lock indicator */
072		assign  imm bit gate0,
073		assign  imm bit gate1,
074		assign  imm bit gate2,
075		assign  imm bit gate3,
076		assign  imm bit gate4,
077		assign  imm bit gate5,
078		assign  imm bit gate6,
079		assign  imm bit gate7,
080		assign  imm bit gate8,
081		assign  imm bit gate9,
082			imm int code,	/* gate code 0 .. 9  - for parcel destination */
083			imm bit p_cell,	/* photo cell monitoring parcel onto belt */
084			imm bit strobe,	/* strobe pulse from belt movement */
085	    )
086	{
087	    imm int car0;		/* carry bit: 0 */
088	    imm int car1;		/* carry bit: 1 */
089	    imm int car2;		/* carry bit: 2 */
090	    imm int car3;		/* carry bit: 3 */
091	    imm int car4;		/* carry bit: 4 */
092	    imm int car5;		/* carry bit: 5 */
093	    imm int car6;		/* carry bit: 6 */
094	    imm int car7;		/* carry bit: 7 */
095	    imm int car8;		/* carry bit: 8 */
096	    imm int car9;		/* carry bit: 9 */
097	    imm int car10;		/* carry bit: 10 */
098
099	    imm clock clk = CLOCK(strobe);

	belt@clk_1  C ---:  belt@clk   :

	iClock      : ---|  belt@clk_1 C
	belt@strobe   ---|

100
101	    lock   = feeder(p_cell, car0, code, 12, 11, clk);

	belt@pip_1   E ---/  belt@pip

	belt@clk     : ---&  belt@pip_1   E
	belt@p_cell    ---&
	belt@pip_2   ~ ---&
	IX0.7        ~ ---&

	belt@pip_3   S ---#  belt@pip_2
	belt@pip_4   R ---#

	belt@clk     : ---&  belt@pip_3   S
	belt@pip       ---&
	belt@pip_5   ~ ---&

	belt@clk     : ---&  belt@pip_4   R
	belt@pip     ~ ---&
	belt@pip_5     ---&

	belt@pip_6     ---|  belt@pip_5
	IX0.7          ---|

	belt@shift   A ---+  belt@pip_6           (belt@shift        // 1
	belt@mask    A ---+                       &belt@mask         // 2
	                                          )==belt@mask       // 2
	                                          ;                  // (5)


	belt@shift_1 H ---*  belt@shift   A
	belt@shift_2 r ---*

	belt@clk     : ---+  belt@shift_1 H
	belt@shift_3 A ---+                       belt@shift_3       // 1
	belt@shift   A ---+                       ?belt@shift        // 2
	belt@shift_4 A ---+                       :belt@shift_4      // 3
	                                          ;                  // (1)

	iClock       : ---|  belt@shift_2 r
	IX0.7          ---|

	IX0.7          ---|  belt@shift_3 A

	belt@shift   A ---+  belt@shift_4 A       (belt@shift        // 1
	belt@shift_5 A ---+                       <<1)+(belt@shift_5 // 2
	belt@code    A ---+                       *(0x41+(belt@code  // 3
	                                          <<1)));            // (2)

	belt@pip       ---|  belt@shift_5 A


	iConst       A ---+  belt@mask    A       0x41<<11;          // (8)


	belt@shift   A ---+  belt@car0    A       (belt@shift        // 1
	                                          >>12)&1;           // (9)


	belt@pip_2     ---@  belt@lock

102	    gate0 = segment(car0, car1,    0, 12,  7, clk);

	belt@shift1_1 H ---*  belt@shift1   A
	belt@shift1_2 r ---*

	belt@clk      : ---+  belt@shift1_1 H
	belt@shift1_3 A ---+                        belt@shift1_3        // 1
	belt@shift1   A ---+                        ?belt@shift1         // 2
	belt@shift1_4 A ---+                        :belt@shift1_4       // 3
	                                            ;                    // (1)

	iClock        : ---|  belt@shift1_2 r
	IX0.7           ---|

	IX0.7           ---|  belt@shift1_3 A

	belt@shift1   A ---+  belt@shift1_4 A       (belt@shift1         // 1
	belt@car0     A ---+                        <<1)+belt@car0       // 2
	                                            ;                    // (6)


	iConst        A ---+  belt@mask1    A       0x41<<7;             // (10)


	belt@shift1   A ---+  belt@car1     A       (belt@shift1         // 1
	                                            >>12)&1;             // (9)


	belt@gate0_1  S ---#  belt@gate0
	belt@gate0_2  R ---#

	belt@clk      : ---&  belt@gate0_1  S
	belt@gate0_3    ---&
	belt@gate0_4  ~ ---&

	belt@clk      : ---&  belt@gate0_2  R
	belt@gate0_3  ~ ---&
	belt@gate0_4    ---&

	belt@shift1   A ---+  belt@gate0_3          (belt@shift1         // 1
	                                            &0x7f)==0x41+(0<<1); // (11)

	belt@gate0_5    ---|  belt@gate0_4
	IX0.7           ---|

	belt@shift1   A ---+  belt@gate0_5          (belt@shift1         // 1
	belt@mask1    A ---+                        &belt@mask1          // 2
	                                            )==belt@mask1        // 2
	                                            ;                    // (5)

103	    gate1 = segment(car1, car2,    1, 12,  7, clk);

	belt@shift2_1 H ---*  belt@shift2   A
	belt@shift2_2 r ---*

	belt@clk      : ---+  belt@shift2_1 H
	belt@shift2_3 A ---+                        belt@shift2_3        // 1
	belt@shift2   A ---+                        ?belt@shift2         // 2
	belt@shift2_4 A ---+                        :belt@shift2_4       // 3
	                                            ;                    // (1)

	iClock        : ---|  belt@shift2_2 r
	IX0.7           ---|

	IX0.7           ---|  belt@shift2_3 A

	belt@shift2   A ---+  belt@shift2_4 A       (belt@shift2         // 1
	belt@car1     A ---+                        <<1)+belt@car1       // 2
	                                            ;                    // (6)


	iConst        A ---+  belt@mask2    A       0x41<<7;             // (10)


	belt@shift2   A ---+  belt@car2     A       (belt@shift2         // 1
	                                            >>12)&1;             // (9)


	belt@gate1_1  S ---#  belt@gate1
	belt@gate1_2  R ---#

	belt@clk      : ---&  belt@gate1_1  S
	belt@gate1_3    ---&
	belt@gate1_4  ~ ---&

	belt@clk      : ---&  belt@gate1_2  R
	belt@gate1_3  ~ ---&
	belt@gate1_4    ---&

	belt@shift2   A ---+  belt@gate1_3          (belt@shift2         // 1
	                                            &0x7f)==0x41+(1<<1); // (12)

	belt@gate1_5    ---|  belt@gate1_4
	IX0.7           ---|

	belt@shift2   A ---+  belt@gate1_5          (belt@shift2         // 1
	belt@mask2    A ---+                        &belt@mask2          // 2
	                                            )==belt@mask2        // 2
	                                            ;                    // (5)

104	    gate2 = segment(car2, car3,    2, 12,  7, clk);

	belt@shift3_1 H ---*  belt@shift3   A
	belt@shift3_2 r ---*

	belt@clk      : ---+  belt@shift3_1 H
	belt@shift3_3 A ---+                        belt@shift3_3        // 1
	belt@shift3   A ---+                        ?belt@shift3         // 2
	belt@shift3_4 A ---+                        :belt@shift3_4       // 3
	                                            ;                    // (1)

	iClock        : ---|  belt@shift3_2 r
	IX0.7           ---|

	IX0.7           ---|  belt@shift3_3 A

	belt@shift3   A ---+  belt@shift3_4 A       (belt@shift3         // 1
	belt@car2     A ---+                        <<1)+belt@car2       // 2
	                                            ;                    // (6)


	iConst        A ---+  belt@mask3    A       0x41<<7;             // (10)


	belt@shift3   A ---+  belt@car3     A       (belt@shift3         // 1
	                                            >>12)&1;             // (9)


	belt@gate2_1  S ---#  belt@gate2
	belt@gate2_2  R ---#

	belt@clk      : ---&  belt@gate2_1  S
	belt@gate2_3    ---&
	belt@gate2_4  ~ ---&

	belt@clk      : ---&  belt@gate2_2  R
	belt@gate2_3  ~ ---&
	belt@gate2_4    ---&

	belt@shift3   A ---+  belt@gate2_3          (belt@shift3         // 1
	                                            &0x7f)==0x41+(2<<1); // (13)

	belt@gate2_5    ---|  belt@gate2_4
	IX0.7           ---|

	belt@shift3   A ---+  belt@gate2_5          (belt@shift3         // 1
	belt@mask3    A ---+                        &belt@mask3          // 2
	                                            )==belt@mask3        // 2
	                                            ;                    // (5)

105	    gate3 = segment(car3, car4,    3, 12,  7, clk);

	belt@shift4_1 H ---*  belt@shift4   A
	belt@shift4_2 r ---*

	belt@clk      : ---+  belt@shift4_1 H
	belt@shift4_3 A ---+                        belt@shift4_3        // 1
	belt@shift4   A ---+                        ?belt@shift4         // 2
	belt@shift4_4 A ---+                        :belt@shift4_4       // 3
	                                            ;                    // (1)

	iClock        : ---|  belt@shift4_2 r
	IX0.7           ---|

	IX0.7           ---|  belt@shift4_3 A

	belt@shift4   A ---+  belt@shift4_4 A       (belt@shift4         // 1
	belt@car3     A ---+                        <<1)+belt@car3       // 2
	                                            ;                    // (6)


	iConst        A ---+  belt@mask4    A       0x41<<7;             // (10)


	belt@shift4   A ---+  belt@car4     A       (belt@shift4         // 1
	                                            >>12)&1;             // (9)


	belt@gate3_1  S ---#  belt@gate3
	belt@gate3_2  R ---#

	belt@clk      : ---&  belt@gate3_1  S
	belt@gate3_3    ---&
	belt@gate3_4  ~ ---&

	belt@clk      : ---&  belt@gate3_2  R
	belt@gate3_3  ~ ---&
	belt@gate3_4    ---&

	belt@shift4   A ---+  belt@gate3_3          (belt@shift4         // 1
	                                            &0x7f)==0x41+(3<<1); // (14)

	belt@gate3_5    ---|  belt@gate3_4
	IX0.7           ---|

	belt@shift4   A ---+  belt@gate3_5          (belt@shift4         // 1
	belt@mask4    A ---+                        &belt@mask4          // 2
	                                            )==belt@mask4        // 2
	                                            ;                    // (5)

106	    gate4 = segment(car4, car5,    4, 12,  7, clk);

	belt@shift5_1 H ---*  belt@shift5   A
	belt@shift5_2 r ---*

	belt@clk      : ---+  belt@shift5_1 H
	belt@shift5_3 A ---+                        belt@shift5_3        // 1
	belt@shift5   A ---+                        ?belt@shift5         // 2
	belt@shift5_4 A ---+                        :belt@shift5_4       // 3
	                                            ;                    // (1)

	iClock        : ---|  belt@shift5_2 r
	IX0.7           ---|

	IX0.7           ---|  belt@shift5_3 A

	belt@shift5   A ---+  belt@shift5_4 A       (belt@shift5         // 1
	belt@car4     A ---+                        <<1)+belt@car4       // 2
	                                            ;                    // (6)


	iConst        A ---+  belt@mask5    A       0x41<<7;             // (10)


	belt@shift5   A ---+  belt@car5     A       (belt@shift5         // 1
	                                            >>12)&1;             // (9)


	belt@gate4_1  S ---#  belt@gate4
	belt@gate4_2  R ---#

	belt@clk      : ---&  belt@gate4_1  S
	belt@gate4_3    ---&
	belt@gate4_4  ~ ---&

	belt@clk      : ---&  belt@gate4_2  R
	belt@gate4_3  ~ ---&
	belt@gate4_4    ---&

	belt@shift5   A ---+  belt@gate4_3          (belt@shift5         // 1
	                                            &0x7f)==0x41+(4<<1); // (15)

	belt@gate4_5    ---|  belt@gate4_4
	IX0.7           ---|

	belt@shift5   A ---+  belt@gate4_5          (belt@shift5         // 1
	belt@mask5    A ---+                        &belt@mask5          // 2
	                                            )==belt@mask5        // 2
	                                            ;                    // (5)

107	    gate5 = segment(car5, car6,    5, 12,  7, clk);

	belt@shift6_1 H ---*  belt@shift6   A
	belt@shift6_2 r ---*

	belt@clk      : ---+  belt@shift6_1 H
	belt@shift6_3 A ---+                        belt@shift6_3        // 1
	belt@shift6   A ---+                        ?belt@shift6         // 2
	belt@shift6_4 A ---+                        :belt@shift6_4       // 3
	                                            ;                    // (1)

	iClock        : ---|  belt@shift6_2 r
	IX0.7           ---|

	IX0.7           ---|  belt@shift6_3 A

	belt@shift6   A ---+  belt@shift6_4 A       (belt@shift6         // 1
	belt@car5     A ---+                        <<1)+belt@car5       // 2
	                                            ;                    // (6)


	iConst        A ---+  belt@mask6    A       0x41<<7;             // (10)


	belt@shift6   A ---+  belt@car6     A       (belt@shift6         // 1
	                                            >>12)&1;             // (9)


	belt@gate5_1  S ---#  belt@gate5
	belt@gate5_2  R ---#

	belt@clk      : ---&  belt@gate5_1  S
	belt@gate5_3    ---&
	belt@gate5_4  ~ ---&

	belt@clk      : ---&  belt@gate5_2  R
