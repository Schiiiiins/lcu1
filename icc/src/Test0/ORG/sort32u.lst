115	/********************************************************************
116	 *
117	 *	Generate tick
118	 *	input fast1 or fast2 cause 50 ms ticks
119	 *	else tick for every change of manual input
120	 *
121	 *******************************************************************/
122
123	imm bit tick(bit manual, bit fast1, bit fast2) {
124	    imm bit fast = fast1 | fast2;

	tick@fast1   ---|  tick@fast
	tick@fast2   ---|

125	    this = CHANGE(manual & ~fast | TX0.4 & fast);

	tick@_1     V --->  tick@

	iClock      : ---|  tick@_1 V
	tick@_2       ---|
	tick@_3       ---|

	tick@manual   ---&  tick@_2
	tick@fast   ~ ---&

	TX0.4         ---&  tick@_3
	tick@fast     ---&

126	}
