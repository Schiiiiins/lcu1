	belt@shift7   A ---+  belt@gate6_3          (belt@shift7         // 1
	                                            &0x7f)==0x41+(6<<1); // (17)

	belt@gate6_5    ---|  belt@gate6_4
	IX0.7           ---|

	belt@shift7   A ---+  belt@gate6_5          (belt@shift7         // 1
	belt@mask7    A ---+                        &belt@mask7          // 2
	                                            )==belt@mask7        // 2
	                                            ;                    // (5)

109	    gate7 = segment(car7, car8,    7, 12,  7, clk);

	belt@shift8_1 H ---*  belt@shift8   A
	belt@shift8_2 r ---*

	belt@clk      : ---+  belt@shift8_1 H
	belt@shift8_3 A ---+                        belt@shift8_3        // 1
	belt@shift8   A ---+                        ?belt@shift8         // 2
	belt@shift8_4 A ---+                        :belt@shift8_4       // 3
	                                            ;                    // (1)

	iClock        : ---|  belt@shift8_2 r
	IX0.7           ---|

	IX0.7           ---|  belt@shift8_3 A

	belt@shift8   A ---+  belt@shift8_4 A       (belt@shift8         // 1
	belt@car7     A ---+                        <<1)+belt@car7       // 2
	                                            ;                    // (6)


	iConst        A ---+  belt@mask8    A       0x41<<7;             // (10)


	belt@shift8   A ---+  belt@car8     A       (belt@shift8         // 1
	                                            >>12)&1;             // (9)


	belt@gate7_1  S ---#  belt@gate7
	belt@gate7_2  R ---#

