108	/********************************************************************
109	 *
110	 *	a moebius ring counter clocked by variable CLOCK 'c'
111	 *	visible on output bank 1
112	 *
113	 *******************************************************************/
114
115	m0 = D(~m7, c);

	m0_1    D ---#  m0

	c       : ---|  m0_1    D
	m7      ~ ---|

116	m1 = D( m0, c);

	m1_1    D ---#  m1

	c       : ---|  m1_1    D
	m0        ---|

117	m2 = D( m1, c);

	m2_1    D ---#  m2

	c       : ---|  m2_1    D
	m1        ---|

118	m3 = D( m2, c);

	m3_1    D ---#  m3

	c       : ---|  m3_1    D
	m2        ---|

119	m4 = D( m3, c);

	m4_1    D ---#  m4

	c       : ---|  m4_1    D
	m3        ---|

120	m5 = D( m4, c);

	m5_1    D ---#  m5

	c       : ---|  m5_1    D
	m4        ---|

121	m6 = D( m5, c);

	m6_1    D ---#  m6

	c       : ---|  m6_1    D
	m5        ---|

122	m7 = D( m6, c);

	m7_1    D ---#  m7

	c       : ---|  m7_1    D
	m6        ---|

